{
  "name": "gymbeam.vue",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "yarn lint:eslint && yarn lint:vue-tsc",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:vue-tsc": "vue-tsc --noEmit",
    "commit": "cz",
    "postinstall": "husky install & npx rimraf ./node_modules/@types/react",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@vueuse/components": "^8.6.0",
    "eslint-plugin-tailwindcss": "^3.5.2",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@mdx-js/react": "^2.1.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-docs": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/vue3": "^6.5.6",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.3",
    "pinst": "^3.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "storybook-dark-mode": "^1.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  },
  "packageManager": "yarn@3.2.1",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
